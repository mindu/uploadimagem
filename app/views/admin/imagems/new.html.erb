<h1>New imagem</h1>

<% form_for(@imagem, :url => admin_imagems_path, :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nome %><br />
    <%= f.text_field :nome %>
  </p>
  <p>
    <%= f.label 'Upload' %><br />
    <%= f.file_field :avatar %>
  </p>
  
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_imagems_path %>
