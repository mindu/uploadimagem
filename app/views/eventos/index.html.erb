<h1>Listing eventos</h1>

<table border="0">
  <tr>
    <th>Data</th>  	
    <th>Titulo</th>
    <td>&nbsp;</td>
  </tr>

<% for evento in @eventos %>
  <tr>
		<td><%=h evento.data.strftime("%d/%m/%Y") %></td>  	
    <td><%=h evento.titulo %></td>
    <td><%= link_to 'Show', evento %></td>
  </tr>
<% end %>
</table>

<br />